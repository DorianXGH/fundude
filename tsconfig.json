{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,

    "esModuleInterop": true
  }
}
